{
  "name": "edigeo2geojson",
  "version": "0.1.0",
  "author": "Jérôme Desboeufs <jerome.desboeufs@data.gouv.fr>",
  "license": "MIT",
  "bin": {
    "edigeo2geojson": "./bin/cli.js"
  },
  "scripts": {
    "lint": "xo"
  },
  "dependencies": {
    "@turf/turf": "^4.5.2",
    "JSONStream": "^1.3.1",
    "ascii-progress": "^1.0.5",
    "bluebird": "^3.4.7",
    "commander": "^2.11.0",
    "debug": "^2.6.1",
    "decompress": "^4.0.0",
    "gdal": "^0.9.6",
    "lodash": "^4.17.4",
    "mkdirp": "^0.5.1",
    "moment": "^2.18.1",
    "rimraf": "^2.6.1",
    "split": "^1.0.0"
  },
  "devDependencies": {
    "xo": "^0.18.2"
  },
  "xo": {
    "space": 2,
    "semicolon": false,
    "rules": {
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "curly": "off",
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "padded-blocks": "off",
      "no-multiple-empty-lines": [
        "error",
        {
          "max": 2
        }
      ]
    }
  },
  "engines": {
    "node": ">= 8"
  }
}
