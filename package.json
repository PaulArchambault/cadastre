{
  "name": "@etalab/cadastre",
  "version": "0.8.1",
  "publishConfig": {
    "access": "public"
  },
  "author": "Jérôme Desboeufs <jerome.desboeufs@data.gouv.fr>",
  "homepage": "https://github.com/etalab/cadastre",
  "repository": {
    "type": "git",
    "url": "https://github.com/etalab/cadastre.git"
  },
  "bugs": {
    "url": "https://github.com/etalab/cadastre/issues"
  },
  "license": "MIT",
  "bin": {
    "cadastre-builder": "./bin/cli.js"
  },
  "scripts": {
    "lint": "xo"
  },
  "dependencies": {
    "@turf/turf": "^4.6.1",
    "JSONStream": "^1.3.1",
    "ascii-progress": "^1.0.5",
    "bluebird": "^3.4.7",
    "commander": "^2.11.0",
    "debug": "^2.6.1",
    "decompress": "^4.0.0",
    "gdal": "^0.9.6",
    "lodash": "^4.17.4",
    "mississippi": "^1.3.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.18.1",
    "rimraf": "^2.6.1",
    "split": "^1.0.0",
    "update-notifier": "^2.2.0",
    "worker-farm": "^1.5.0"
  },
  "devDependencies": {
    "xo": "^0.18.2"
  },
  "xo": {
    "space": 2,
    "semicolon": false,
    "rules": {
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "curly": "off",
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "padded-blocks": "off",
      "no-multiple-empty-lines": [
        "error",
        {
          "max": 2
        }
      ],
      "capitalized-comment": "off"
    }
  },
  "engines": {
    "node": ">= 8"
  }
}
